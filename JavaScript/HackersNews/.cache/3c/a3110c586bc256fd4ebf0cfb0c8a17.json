if (routePath === \"\") {\n    loadNewsFeed();\n  } else if (routePath.indexOf(\"#/page/\") >= 0) {\n    store.currentPage = Number(routePath.substring(7));\n    loadNewsFeed();\n  } else {\n    loadNewsDetail();\n  }\n}\n\nwindow.addEventListener(\"hashchange\", router);\n\nrouter();\n"},"lineCount":null}},"error":null,"hash":"5b6973f28285157b637bebff9c5f7d03","cacheData":{"env":{}}}